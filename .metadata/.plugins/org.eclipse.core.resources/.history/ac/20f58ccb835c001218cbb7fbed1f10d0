#include <iostream>
#include <stdlib.h>

template <class Item>
	void exch(Item &A, Item &B){
		Item t = A; A = B; B = t;
	}

template <class Item>
	void compexch(Item &A, Item &B){
		if (B < A) exch(A, B);
	}

template <class Item>
	void sort(Item a[], int l, int r){

	}

