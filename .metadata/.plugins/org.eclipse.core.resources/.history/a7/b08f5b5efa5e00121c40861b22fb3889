#include <iostream>
#include <stdlib.h>
#include "IncreSTACK.cpp"

using namespace std;

//(1) TRISTACK
//(2) MinSTACK
//(3) IncreSTACK

//(4)

void hanoi(int N, int tower){
	hanoi(N - 1, (tower + 1) % 2);
	if(N == 1)return -1;
	hanoi(N - 1, (tower + 1) % 2);


}

int main(int argc, char * argv[]){
	INCRESTACK<int> stack;


}
