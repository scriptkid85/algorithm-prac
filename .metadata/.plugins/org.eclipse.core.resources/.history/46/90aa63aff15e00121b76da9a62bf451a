#include <iostream>
#include <stdlib.h>
#include "TRISTACK.cpp"
#include "MinSTACK.cpp"

using namespace std;
//(1)
//TriSTACK in 3STACK.cpp


//(2)
//MinSTACK in MinSTACK.cpp

//(3)


int main(int argc, char *argv[]){
	TriSTACK<int> tristack(10);
	MinSTACK<int> minstack(10);

	for(int i = 0; i < 5; i++)
	tristack.push(0, i + 1);

	for(int i = 0; i < 5; i++)
	tristack.push(1, i + 6);

	for(int i = 0; i < 5; i++)
	tristack.push(2, i + 11);

	for(int i = 0; i < 5; i++)
		cout << tristack.pop(0) << " " << tristack.pop(1) << " " << tristack.pop(2) << endl;

	minstack.push(10);
	minstack.push(5);
	minstack.push(6);
	minstack.push(9);
	minstack.push(1);


}
