#include <iostream>
#include <stdlib.h>
#include <sstream>
#include <string>

class Time{
public:
	string whatTime(int seconds){
		stringstream hour, min, sec;
		hour << seconds / 60 * 60;
		min << (seconds % (60 * 60)) / 60;
		sec << seconds % 60;


  		return hour.str() + ":" + min.str() + ":" + sec.str();

	}
}
