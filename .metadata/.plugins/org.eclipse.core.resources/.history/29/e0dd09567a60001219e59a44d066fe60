int findloss(vector<int*> array){
	if(array.size() == 0)return 0;

	int n = array.size();
	vector<int*> zeros, ones;
	int position = 0;
	while(array.size() > 0){
		for(vector<int *>::iterator it = array.begin(); it != array.end() ; ++it){
			if((*it)[position] == 0){
				ones.push_back((*it));
				onecnt ++;
			}
		}
	}
}

